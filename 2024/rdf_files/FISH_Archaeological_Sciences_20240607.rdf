    <skos:scopeNote xml:lang="en">A process using an electron microscope in which the surface of the specimen is scanned by a beam of electrons which are reflected to form an image. Very high magnification is possible.</skos:scopeNote>
  </skos:Concept>

  <skos:Concept rdf:about="http://purl.org/heritagedata/schemes/560/concepts/142203">
    <skos:altLabel xml:lang="en">TL Dating</skos:altLabel>
