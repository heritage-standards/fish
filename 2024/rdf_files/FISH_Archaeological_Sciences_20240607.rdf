  <skos:Concept rdf:about="http://purl.org/heritagedata/schemes/560/concepts/142122">
    <skos:altLabel xml:lang="en">Bulk Sampling</skos:altLabel>
    <skos:broader rdf:resource="http://purl.org/heritagedata/schemes/560/concepts/142162"/>
    <skos:inScheme rdf:resource="http://purl.org/heritagedata/schemes/560"/>
    <dc:identifier>http://purl.org/heritagedata/schemes/560/concepts/142122</dc:identifier>
    <dc:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2024-06-07T14:22:55</dc:issued>
    <cc:license rdf:resource="https://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/"/>
